      "command": "uv",
      "args": [
        "--directory",
        "/home/ygg/Workspace/RegenAI/regen-registry-review-mcp",
        "run",
        "python",
        "-m",
        "registry_review_mcp.server"
      ],
      "env": {
        "REGISTRY_REVIEW_LOG_LEVEL": "INFO"
      }
    },
    "regen-koi": {
      "command": "node",
      "args": ["/home/ygg/Workspace/sandbox/regen-koi-mcp/dist/index.js"],
